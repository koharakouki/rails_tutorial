<%= search_form_for @q do |f| %>
	<%= f.label :name_cont, 'User Serch' %>
	<div class="input-group">
		<%= f.text_field :name_cont, placeholder: "Enter username...",
							class: "form_control" %>
		<span class="input-group-btn">
			<%= f.submit 'Go', class: "btn btn-primary" %>
		</span>
	</div>
<% end %>